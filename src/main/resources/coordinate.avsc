{
   "namespace": "com.belykh.kafka.avro",
   "type": "record",
   "name": "Coordinate",
   "fields": [
      {"name": "id", "type": "int"},
      {"name": "lat", "type": "double"},
      {"name": "lon", "type": "double"},
      {"name": "speed", "type": "double", "default": 1.0},
      {
        "name": "datetime",
        "type": [
            "null",
            {"type": "long", "logicalType": "local-timestamp-millis"}
        ],
        "default": null
      }
   ]
}